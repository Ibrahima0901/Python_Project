//creaation fichier jenkins
pipeline {
agent any
stages {
stage('Recuperation de notre repository sur github') {
steps {
git branch: 'master', url: 'https://github.com/Ibrahima0901/Python_Project.git'
}
}
stage('Install Dependencies') {
steps {
bat'python -m venv venv'
}
}
2
stage('Run Script') {
steps {
bat'.\\venv/bin\\python app.py'
}
}
}

}
